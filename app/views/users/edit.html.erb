<h1> <%=link_to 'moonwalk', welcomes_path, class: "little_welcomer" %> </h1>

<h1 class = "page_title"> Edit <%=@user.name%> </h1>

<div class = "form_container">
<%= form_for @user do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="field">
    <%= form.label :age %>
    <%= form.text_field :age %>
  </div>
  <div class="field">
    <%= form.label :location %>
    <%= form.text_field :location %>
  </div>
  <div class="action">
    <%= form.submit 'Update' %>
  </div>
</div>
<% end %>